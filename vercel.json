{
  "version": 2,
  "builds": [
    {
      "src": "src/utils/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/auth/google/login",
      "dest": "src/utils/server.js"
    },
    {
      "src": "/auth/google/callback",
      "dest": "src/utils/server.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "src/utils/server.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "src/utils/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}