{
  "version": 2,
  "builds": [
    {
      "src": "src/utils/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "src/utils/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/utils/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}